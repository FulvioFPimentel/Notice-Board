package com.bigcrowd.noticeBoard.controllers;

import java.util.List;

import org.springframework.http.ResponseEntity;

import com.bigcrowd.noticeBoard.controllers.services.CanticleService;
import com.bigcrowd.noticeBoard.entities.Canticle;

public class CanticleController {
	
	private CanticleService canticleService;
	
	public ResponseEntity<List<Canticle>> findAll() {
		List<Canticle> canticles = canticleService.findAll();
		return ResponseEntity.ok().body(canticles);		
	}

}
