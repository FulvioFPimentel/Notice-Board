package com.bigcrowd.noticeBoard.entities;

import java.io.Serializable;
import java.util.ArrayList;
import java.util.Date;
import java.util.List;

public class Meeting implements Serializable {
	private static final long serialVersionUID = 1L;
	
	private long id;
	private Date date;
	private String presidency;
	
	
	private List<Canticle> canticles = new ArrayList<>();
	
	private List<Prayer> prayers = new ArrayList<>();


	
	
	public Meeting() {
	}

	public Meeting(long id, String presidency, Date date, List<Canticle> canticles, List<Prayer> prayers) {

		this.canticles = canticles;
		this.prayers = prayers;
		this.presidency = presidency;
		this.date = date;
	}

	public long getId() {
		return id;
	}

	public void setId(long id) {
		this.id = id;
	}

	public List<Canticle> getCanticles() {
		return canticles;
	}

	public void setCanticles(List<Canticle> canticles) {
		this.canticles = canticles;
	}

	public List<Prayer> getPrayers() {
		return prayers;
	}

	public void setPrayers(List<Prayer> prayers) {
		this.prayers = prayers;
	}

	public String getPresidency() {
		return presidency;
	}

	public void setPresidency(String presidency) {
		this.presidency = presidency;
	}

	public Date getDate() {
		return date;
	}

	public void setDate(Date date) {
		this.date = date;
	}
		
	
}
