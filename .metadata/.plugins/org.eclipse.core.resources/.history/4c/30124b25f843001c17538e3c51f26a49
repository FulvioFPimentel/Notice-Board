package com.bigcrowd.noticeBoard.entities;

import java.io.Serializable;

public class Prayer implements Serializable{
	private static final long serialVersionUID = 1L;
	
	private long id;
	private int moment;
	private String peable;
	
	public Prayer() {	
	}

	public Prayer(Long id, int moment, String peable) {
		this.id = id;
		this.moment = moment;
		this.peable = peable;
	}
	

	public long getId() {
		return id;
	}

	public void setId(long id) {
		this.id = id;
	}

	public int getMoment() {
		return moment;
	}

	public void setMoment(int moment) {
		this.moment = moment;
	}

	public String getPeable() {
		return peable;
	}

	public void setPeable(String peable) {
		this.peable = peable;
	}
	
		
}
